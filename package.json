{
  "name": "coc-glint",
  "version": "1.0.4",
  "private": false,
  "description": "Glint language server extension for coc.nvim",
  "main": "lib/extension.js",
  "author": "NullVoxPopuli",
  "license": "MIT",
  "homepage": "https://github.com/NullVoxPopuli/coc-glint",
  "repository": "https://github.com/NullVoxPopuli/coc-glint",
  "keywords": [
    "ember",
    "glimmer",
    "handlebars",
    "typescript",
    "nvim",
    "coc.nvim",
    "glint",
    "ember-language-server"
  ],
  "categories": [
    "Programming Languages",
    "Linters"
  ],
  "files": [
    "lib"
  ],
  "engines": {
    "coc": "^0.0.77"
  },
  "activationEvents": [
    "workspaceContains:.glintrc*"
  ],
  "scripts": {
    "release": "semantic-release",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:js": "eslint .",
    "build": "tsc --skipLibCheck",
    "prepublishOnly": "pnpm run build"
  },
  "release": {
    "branches": [
      "main",
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "dependencies": {
    "@glint/config": "^0.9.0",
    "@glint/core": "^0.9.0",
    "coc.nvim": "^0.0.82",
    "resolve": "^1.22.0"
  },
  "devDependencies": {
    "@nullvoxpopuli/eslint-configs": "^2.2.14",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^18.0.0",
    "@types/resolve": "^1.20.2",
    "semantic-release": "^19.0.2",
    "typescript": "^4.6.4"
  },
  "packageManager": "pnpm@7.28.0",
  "volta": {
    "node": "18.14.2",
    "yarn": "1.22.19"
  }
}
